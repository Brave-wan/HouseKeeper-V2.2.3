<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/base_dimen_28"
        android:background="@color/white"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/base_dimen_100"
            android:layout_marginLeft="@dimen/base_dimen_24"
            android:layout_marginRight="@dimen/base_dimen_24"
            android:gravity="center">

            <TextView
                android:id="@+id/end_handover_room_project_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/end_handover_room_project"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

            <TextView
                android:id="@+id/end_handover_room_project"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_dimen_2"
            android:background="@color/equipment_line_6" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/base_dimen_100"
            android:layout_marginLeft="@dimen/base_dimen_24"
            android:layout_marginRight="@dimen/base_dimen_24"
            android:gravity="center">

            <TextView
                android:id="@+id/handover_room_number_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/handover_room_number"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

            <TextView
                android:id="@+id/handover_room_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_dimen_2"
            android:background="@color/equipment_line_6" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/base_dimen_100"
            android:layout_marginLeft="@dimen/base_dimen_24"
            android:layout_marginRight="@dimen/base_dimen_24"
            android:gravity="center">

            <TextView
                android:id="@+id/handover_room_address_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/handover_room_address"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

            <TextView
                android:id="@+id/handover_room_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_dimen_2"
            android:background="@color/equipment_line_6" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/base_dimen_100"
            android:layout_marginLeft="@dimen/base_dimen_24"
            android:layout_marginRight="@dimen/base_dimen_24"
            android:gravity="center">

            <TextView
                android:id="@+id/handover_room_name_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/handover_room_name"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

            <TextView
                android:id="@+id/handover_room_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_dimen_2"
            android:background="@color/equipment_line_6" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/base_dimen_100"
            android:layout_marginLeft="@dimen/base_dimen_24"
            android:layout_marginRight="@dimen/base_dimen_24"
            android:gravity="center">

            <TextView
                android:id="@+id/handover_room_phone_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/handover_room_phone_text"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

            <TextView
                android:id="@+id/handover_room_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/layout_title_bar_text_bg"
                android:textSize="@dimen/base_dimen_32" />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_dimen_2"
            android:background="@color/equipment_line_6" />

    </LinearLayout>

</RelativeLayout>